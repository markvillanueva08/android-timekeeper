{
  "expo": {
    "name": "Admin Team Timekeeper",
    "slug": "android-timekeeper",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/admin-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/admin-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.markvillanueva1221.androidtimekeeper"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/admin-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c20b90cb-cc84-45c4-868a-3f5b80080f96"
      }
    }
  }
}
